#include<iostream>
using namespace std;
int main(){
int arr[10],n,m,s=0;
cout<<"Enter the number of elements: ";
cin>>n;
cout<<"Enter the array elements: "<<endl;
for(int i=0;i<n;i++){
        cin>>arr[i];

}
cout<<"Enter the search element: ";
cin>>m;
for(int i=0;i<n;i++){
    if (arr[i]==m)
        s+=1;
}
cout<<m<<" appears "<< s <<" times "<<endl;
return 0;

}
